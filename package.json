{
  "name": "mobile-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--no-deprecation' jest"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
